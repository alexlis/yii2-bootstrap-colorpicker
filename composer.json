{
    "name": "alexlis/yii2-bootstrap-colorpicker",
    "description": "Bootstrap colorpicker widget for Yii2.",
    "type": "yii2-extension",
    "keywords": [
        "yii",
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "colorpicker"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "alexlis",
            "email": "mengzhonghua12@gamil.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/alexlis/yii2-bootstrap-colorpicker/issues",
        "source": "https://github.com/alexlis/yii2-bootstrap-colorpicker"
    },
    "minimum-stability": "dev",
    "require": {
        "mjolnic/bootstrap-colorpicker": "2.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "alexlis\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
